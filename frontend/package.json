{
	"name": "skyzar-frontend",
	"version": "0.0.1",
	"private": true,
	"author": {
		"name": "tonydawhale",
		"url": "https://tonydoes.dev/"
	},
	"description": "The frontend for the worlds first free and open source Hypixel Skyblock Bazaar Interface.",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy": "node build/index.js",
		"fix": "prettier --write . && eslint --fix"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.20.4",
		"@types/nprogress": "^0.2.0",
		"@typescript-eslint/eslint-plugin": "^6.1.0",
		"@typescript-eslint/parser": "^6.1.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.32.4",
		"flowbite-svelte-icons": "^0.2.6",
		"postcss": "^8.4.27",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.6",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.4.5",
		"flowbite": "^1.7.0",
		"flowbite-svelte": "^0.40.1",
		"match-sorter": "^6.3.1",
		"nprogress": "^0.2.0",
		"svelte-spotlight": "^1.0.14",
		"tailwind-merge": "^1.14.0"
	}
}
